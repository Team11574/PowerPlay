<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <!--
    In general, hardware component names are formatted
    using the notation SUBSYSTEMPREFIX_componentName_COMPONENTTYPEINITIAL.

    For example, the front right motor on the drive train would be written:
    DT_frontRight_M.

    Subsystem prefixes:
        - DT (drivetrain)
        - VS (vertical slide)
        - HS (horizontal slide)

    Component type initials:
        - M (motor)
        - S (servo)
        - D (digital device)
     -->

    <LynxUsbDevice
        name="Control Hub Portal"
        serialNumber="(embedded)"
        parentModuleAddress="173">
        <LynxModule
            name="Expansion Hub 2"
            port="2">
            <!-- Vertical Slide Motors -->
            <goBILDA5202SeriesMotor
                name="VS_slideRight_M"
                port="0" />
            <goBILDA5202SeriesMotor
                name="VS_slideLeft_M"
                port="1" />

            <!-- Horizontal Slide Motors -->
            <goBILDA5202SeriesMotor
                name="HS_slide_M"
                port="2" />

            <!-- REMOVE THIS AFTER DONE TESTING -->
            <goBILDA5202SeriesMotor
                name="testing_M"
                port="3" />

            <!-- Horizontal Slide Servos -->
            <Servo
                name="HS_claw_S"
                port="0" />
            <Servo
                name="HS_turret_S"
                port="1" />
            <Servo
                name="HS_hinge_S"
                port="2" />
            <Servo
                name="HS_lever_S"
                port="3" />

            <!-- Vertical Slide Limit Switch -->
            <DigitalDevice
                name="VS_limitSwitch_D"
                port="0" />
        </LynxModule>
        <LynxModule
            name="Control Hub"
            port="173">
            <!-- Drive Train Motors -->
            <NeveRest40Gearmotor
                name="DT_frontRight_M"
                port="0" />
            <NeveRest40Gearmotor
                name="DT_backRight_M"
                port="1" />
            <NeveRest40Gearmotor
                name="DT_backLeft_M"
                port="2" />
            <NeveRest40Gearmotor
                name="DT_frontLeft_M"
                port="3" />

            <!-- Vertical Slide Servos -->
            <Servo
                name="VS_flip_S"
                port="0" />
            <Servo
                name="VS_claw_S"
                port="1" />
            <Servo
                name="frontArm"
                port="5" />

            <!-- IMU  -->
            <ControlHubImuBHI260AP
                name="imu"
                port="0"
                bus="0" />

            <REV_VL53L0X_RANGE_SENSOR
                name="HS_distanceSensor"
                port="0"
                bus="1" />
        </LynxModule>
    </LynxUsbDevice>

    <Webcam
        name="camera"
        serialNumber="C4860580" />
</Robot>